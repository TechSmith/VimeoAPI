<Window x:Class="VimeoHarness.Dialogs.LoginDlg"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:Helpers="clr-namespace:VimeoHarness.Helpers"
        mc:Ignorable="d"
        Title="LoginDlg" Height="450" Width="600">
    <Grid>
        <WebBrowser HorizontalAlignment="Stretch"
                   VerticalAlignment="Stretch"
                    Helpers:WebBrowserHelper.BindableSource="{Binding WebAddress}"
                    Navigating="WebBrowser_Navigating"/>
    </Grid>
</Window>
